{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" },
    { "src": "*.jsx", "use": "@vercel/static" },
    { "src": "images/**", "use": "@vercel/static" },
    { "src": "*.png", "use": "@vercel/static" },
    { "src": "*.jpg", "use": "@vercel/static" },
    { "src": "*.svg", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/blog", "dest": "/blog.html" },
    { "src": "/about", "dest": "/about.html" },
    { "src": "/services", "dest": "/services.html" },
    { "src": "/contact", "dest": "/contact.html" },
    { "src": "/what-i-learned", "dest": "/what-i-learned.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
